{
	"version": "0.1",
	"assets": {
		"roots": [
			{
				"name": "data",
				"loaders": [
					{ "type": "RelativeURLLoader", "relPath": "data/", "disableCache": false },
					{ "type": "DataURLLoader" }
				]
			}
		]
	},
	"physics": {
		"ammo": {
			"useWebAssembly": true
		}
	},
	"render": {
		"canvasParentSelector": ".stageholder",
		"initialWidth": 1280,
		"initialHeight": 720
	},
	"load": {
		"preloadScenes": ["base-scene"],
		"initialScene": "base-scene"
	}
}
