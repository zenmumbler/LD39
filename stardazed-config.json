{
	"version": "0.1",
	"assets": {
		"loader": {
			"type": "chain",
			"loaders": [
				{ "type": "data-url" },
				{ "type": "rooted", "prefix": "data", "loader": { "type": "doc-relative-url", "relPath": "data/" } }
			]
		}
	},
	"physics": {
		"ammo": {
			"useWebAssembly": true
		}
	},
	"render": {
		"canvasParentSelector": ".stageholder",
		"initialWidth": 1280,
		"initialHeight": 720
	},
	"load": {
		"preloadScenes": ["base-scene"],
		"initialScene": "base-scene"
	}
}
